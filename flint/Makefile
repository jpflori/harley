CC=gcc
CXX=g++
CFLAGS=-O2 -g -ansi -pedantic -Wall -funroll-loops  -DFLINT_CPIMPORT=\"/media/local/flori/local/share/flint/CPimport.txt\"
PREFIX=/media/local/flori/local

POINTS=points
POINTS_DENSE=points_dense

LIBS=-L$(CURDIR) -L$(PREFIX)/lib -lflint -lmpir -lmpfr -lm
INCS=-I$(CURDIR) -I$(PREFIX)/include -I$(PREFIX)/include/flint

LD_LIBRARY_PATH:=${CURDIR}:${PREFIX}/lib:${LD_LIBRARY_PATH}

export

all: $(POINTS) $(POINTS_DENSE)

clean:
	rm -rf $(POINTS) $(POINTS_DENSE) *.o

distclean: clean
	rm -f Makefile

%: %.c
	$(CC) -fPIC $(CFLAGS) $(INCS) $< -o $@ $(LIBS)

.PHONY: clean all
